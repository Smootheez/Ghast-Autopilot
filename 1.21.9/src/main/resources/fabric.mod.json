{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${modVersion}+${minecraftVersion}",
  "icon": "assets/ghastautopilot/icon.png",
  "name": "${modName}",
  "description": "${modDescription}",
  "license": "${modLicense}",
  "authors": [
    "Smootheez"
  ],
  "contact": {
    "homepage": "https://github.com/Smootheez/Ghast-Autopilot",
    "sources": "https://github.com/Smootheez/Ghast-Autopilot",
    "issues": "https://github.com/Smootheez/Ghast-Autopilot/issues"
  },
  "environment": "client",
  "entrypoints": {
    "main": [
      "io.github.smootheez.ghastautopilot.GhastAutopilot"
    ],
    "client": [
      "io.github.smootheez.ghastautopilot.GhastAutopilotClient"
    ],
    "smoothiezapi": [
      "io.github.smootheez.ghastautopilot.config.GhastAutopilotConfig"
    ]
  },
  "mixins": [
    {
      "config": "ghastautopilot.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loaderVersion}",
    "minecraft": ">=${minecraftVersion}",
    "smoothiezapi": ">=${smoothiezApiVersion}",
    "fabric": ">=${fabricVersion}"
  }
}